# AIDIS Command Frontend Environment Variables
# Copy this file to .env and configure as needed

# Backend REST API URL (aidis-command backend)
# Default: http://localhost:5000/api (development)
REACT_APP_API_URL=http://localhost:5000/api

# MCP Server HTTP Bridge URL
# Default: http://localhost:8080 (development)
REACT_APP_MCP_URL=http://localhost:8080

# WebSocket URL for real-time updates
# Default: ws://localhost:5000/ws (development)
REACT_APP_WS_URL=ws://localhost:5000/ws

# Sentry Error Tracking (Optional)
REACT_APP_SENTRY_DSN=
REACT_APP_SENTRY_ENABLED=false
